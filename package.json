{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "IMessage - Find 2FA Codes",
  "title": "IMessage - Find 2FA Codes",
  "description": "Find 2FA Codes in your messages",
  "icon": "command-icon.png",
  "author": "BalliAsghar",
  "categories": [
    "Productivity",
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "IMessage - Find 2FA Codes",
      "description": "Find 2FA Codes in your recent IMessages",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "minutes",
      "title": "How many minutes to look back?",
      "description": "How many minutes to look back?",
      "type": "textfield",
      "required": false,
      "default": "15"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.35.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
